{
  "manifest_version": 2,
  "name": "Українські новини",
  "description": "Зручний перегляд заголовків останніх новин з найвідоміших українських сайтів",
  "version": "1.4.7",
  "options_page": "options.html",
  "background" : {
    "scripts" : ["common.js","news.js","background.js"]
  },
  "permissions": [
    "http://www.pravda.com.ua/rss/view_news/",
    "http://lb.ua/export/rss.xml",
    "http://news.liga.net/news/rss.xml",
    "http://rss.unian.net/site/news_rus.rss",
    "http://censor.net.ua/includes/news_ru.xml",
    "http://zn.ua/rss",
    "http://news.finance.ua/rss",
    "http://nv.ua/xml/rss.xml",
    "http://tsn.ua/rss",
    "http://ua.interfax.com.ua/news/last.rss",
    "tabs",
    "storage",
    "alarms"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "browser_action": {
    "default_icon": "img/icon48.png",
    "default_popup": "news.html"
  },
  "icons": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
  }
}