{
  "manifest_version": 2,
  "name": "Українські новини",
  "description": "Зручний перегляд заголовків останніх новин з найвідоміших українських сайтів",
  "version": "1.5.4",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "common.js",
      "news.js",
      "background.js"
    ]
  },
  "permissions": [
    "https://www.pravda.com.ua/rss/view_news/",
    "https://lb.ua/rss/ukr/rss.xml",
    "http://news.liga.net/news/rss.xml",
    "http://rss.unian.net/site/news_rus.rss",
    "https://ua.censor.net.ua/includes/news_uk.xml",
    "https://dt.ua/rss",
    "https://news.finance.ua/rss",
    "https://nv.ua/xml/rss.xml",
    "https://tsn.ua/rss",
    "http://ua.interfax.com.ua/news/last.rss",
    "http://k.img.com.ua/rss/ru/all_news2.0.xml",
    "https://gazeta.ua/rss",
    "https://112.ua/rss/index.rss",
    "https://espreso.tv/rss",
    "https://gordonua.com/xml/rss.html",
    "storage",
    "alarms"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "browser_action": {
    "default_icon": "img/icon48.png",
    "default_popup": "news.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}